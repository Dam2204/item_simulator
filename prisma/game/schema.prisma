generator client {
  provider = "prisma-client-js"
  output = "./generated/GameDataClient"
}

datasource gamedb {
  provider = "mysql"
  url      = env("DATABASE_URL1")
}


model Items {
  id          Int      @id @default (autoincrement())
  item_code   Int      @unique @map("item_code")
  item_name   String   @unique @map("item_name")
  health      Int      @map("health")
  power       Int      @map("power")
  item_price  Int      @map("item_price")

  @@map("Items")
}
